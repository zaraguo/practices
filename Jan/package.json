{
  "name": "Jan",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "gulp",
    "test": "gulp test"
  },
  "babel": {
    "presets": [
      "latest",
      "react",
      "flow"
    ],
    "compact": false
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0-beta.6",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-flow": "^1.0.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "eslint": "^3.8.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-flowtype": "^1.0.0",
    "gulp-mocha": "^3.0.1",
    "mocha": "^3.2.0",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "webpack-stream": "^3.2.0"
  },
  "eslintConfig": {
    "extends": [
        "airbnb",
        "plugin:flowtype/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "flowtype"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "new-cap": [
        2,
        {
          "capIsNewExceptions": [
            "Map",
            "List"
          ]
        }
      ]
    },
    "env": {
      "browser": true
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "immutable": "^3.8.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-immutable": "^3.0.9"
  }
}
